---
import BaseLayout from "../layouts/BaseLayout.astro";

Astro.response.status = 410;
---
<BaseLayout
  title="Page removed — Nexusario"
  description="This page has been permanently removed."
>
  <section class="nf-wrap nf-full">
    <div class="nf-inner">
      <p class="nf-eyebrow" style="font-size: large;">410 Gone</p><br />

        <h1 class="nf-title">
          This page has been permanently removed.
        </h1><br />


        <p class="nf-desc">
          The content you’re looking for is no longer available.
        </p><br />


        <a href="/" class="nf-btn">
          Go to Home
        </a>
      </div>
    </section>

  <meta name="robots" content="noindex" />
</BaseLayout>
