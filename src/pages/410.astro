---
import BaseLayout from "../layouts/BaseLayout.astro";

Astro.response.status = 410;
---
<BaseLayout
  title="Page removed — Nexusario"
  description="This page has been permanently removed."
>
  <div class="nf-page">
    <section class="nf-wrap nf-null">
      <div class="nf-inner">
        <p class="nf-eyebrow">410 Gone</p>

        <h1 class="nf-title">
          This page has been permanently removed.
        </h1>

        <p class="nf-desc">
          The content you’re looking for is no longer available.
        </p>

        <a href="/" class="nf-btn">
          Go to Home
        </a>
      </div>
    </section>
  </div>

  <meta name="robots" content="noindex" />
</BaseLayout>
